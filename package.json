{
  "scripts": {
    "dev": "concurrently \"npm:server\" \"npm:watch\" \"npm:mail-hog\"",
    "mail-hog": "~/go/bin/MailHog",
    "server": "php -S localhost:8888 -t public server.php",
    "watch": "kirbyup serve src/index.js",
    "build": "parcel build src/index.js --no-source-maps --experimental-scope-hoisting -d ./"
  },
  "devDependencies": {
    "@prettier/plugin-php": "^0.19.1",
    "concurrently": "^6.2.0",
    "kirbyup": "^2.0.1",
    "prettier": "^2.7.1"
  }
}
